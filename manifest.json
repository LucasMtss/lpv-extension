{
  "name": "Biblioteca IF Sudeste",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "plugin para alterar a UI do site da biblioteca do IF Sudeste",
  "web_accessible_resources": [
    {
      "resources": [
        "bg-image.png",
        "icon-menu.png",
        "logo-if.png",
        "styles.css",
        "results.css",
        "video-home.mp4",
        "video-login.mp4"
      ],
      "matches": [
        "http://biblioteca.barbacena.ifsudestemg.edu.br/*",
        "https://www.loom.com/share/a7e54e4dbf614e6aaec4029a1c415f67"
      ]
    }
  ],
  "content_scripts": [
    {
      "js": ["init.js"],
      "css": ["styles.css"],
      "run_at": "document_idle",
      "matches": [
        "http://biblioteca.barbacena.ifsudestemg.edu.br/cgi-bin/wxis.exe?IsisScript=phl82.xis&cipar=phl82.cip&lang=por"
      ]
    }
  ]
}
